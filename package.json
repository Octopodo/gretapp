{
  "name": "gretapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "kt": "node ./scripts/create-components.js kt",
    "remove": "node ./scripts/create-components.js remove",
    "import-sprites": "node ./scripts/import-sprites.js import-sprites --commit",
    "delete-sprites": "node ./scripts/import-sprites.js delete-sprites --commit",
    "remove-sprites": "node ./scripts/import-sprites.js delete-sprites --commit"
  },
  "dependencies": {
    "@types/animejs": "^3.1.12",
    "@vueuse/core": "^10.7.1",
    "@vueuse/sound": "^2.0.1",
    "commander": "^12.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.5",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "animejs": "^3.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.25"
  }
}
